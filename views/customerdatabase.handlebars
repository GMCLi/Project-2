<div id="home">
  <div class="container">
    <div class="row">
      <div class="col-8 offset-2">
        <h1>{{msg}}</h1>
        <h3>Customers:</h3>
        <ul id="example-list" class="list-group">
          {{#each customer}}
          <li data-id="{{this.id}}" class="list-group-item">
            <a href="customer/{{this.dataValues.id}}">{{this.dataValues.customerName}}</a>
            <button class="btn btn-warning float-right update"
              onclick="location.href = 'http://localhost:3000/customer/{{this.dataValues.id}}/customerupdate';">Update</button>
            <button class="btn btn-danger float-right delete"><i class="icon-remove"></i>Delete</button>
          </li>
          {{/each}}
        </ul>
      </div>
    </div>
  </div>
</div>

<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="/js/index.js"></script>